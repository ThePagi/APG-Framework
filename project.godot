config_version=5

[application]

config/name="APG Framework"
run/main_scene="res://webrtc/template.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

APGGameSettings="*res://addons/APG/Scripts/AutoLoad/GameSettings.gd"
APGUserSettings="*res://addons/APG/Scripts/AutoLoad/UserSettings.gd"
APGNetwork="*res://addons/APG/Scenes/AutoLoad/Network.tscn"
APGSurveyDB="*res://addons/APG/Scenes/AutoLoad/SurveyDB.tscn"

[debug]

gdscript/warnings/integer_division=0
shapes/collision/shape_color=Color(0.960784, 0, 0.505882, 0.419608)

[display]

window/size/viewport_width=480
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[dotnet]

project/assembly_name="StreamGames"

[editor_plugins]

enabled=PackedStringArray("res://addons/APG/plugin.cfg")

